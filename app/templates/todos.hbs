<section id="todoapp">

  <header id="header">
    <h1>todos</h1>
    <input type="text" id="new-todo" placeholder="What needs to be done?" autofocus="">
  </header>


  <section id="main">
    <ul id="todo-list">
      <li class="completed">
        <input type="checkbox" class="toggle">
        <label>Profit</label><button class="destroy"></button>
      </li>
      <li>
        <input type="checkbox" class="toggle">
        <label>Get Milk</label><button class="destroy"></button>
      </li>
      <li>
        <input type="checkbox" class="toggle">
        <label>Get Fries</label><button class="destroy"></button>
      </li>
    </ul>
  </section>

  <footer id="footer">
    <span id="todo-count"><strong>0</strong> items left</span>
    <ul id="filters">
      <li><a href="#">All</a></li>
      <li><a href="#">Active</a></li>
      <li><a href="#">Completed</a></li>
    </ul>
    <button id="clear-completed">Clear completed</button>
  </footer>

</section>